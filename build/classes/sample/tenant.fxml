<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.tenantController">
   <children>
      <HBox prefHeight="67.0" prefWidth="600.0">
         <children>
            <TextField prefHeight="35.0" prefWidth="160.0">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
            <ChoiceBox prefHeight="35.0" prefWidth="160.0">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </ChoiceBox>
            <Button mnemonicParsing="false" prefHeight="35.0" prefWidth="100.0" text="Search">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <VBox layoutX="-3.0" layoutY="67.0" prefHeight="153.0" prefWidth="600.0">
         <children>
            <TableView prefHeight="111.0" prefWidth="590.0">
              <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
              </columns>
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
            </TableView>
         </children>
      </VBox>
      <Button fx:id="addBtn" layoutX="25.0" layoutY="220.0" mnemonicParsing="false" onAction="#handleaddTenant" prefHeight="42.0" prefWidth="107.0" text="add Tenant" />
      <Button layoutX="172.0" layoutY="220.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="112.0" text="update Tenant" />
      <Button layoutX="325.0" layoutY="220.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="112.0" text="delete Tenant" />
   </children>
</AnchorPane>
